            </toolkit:NavigationOutTransition.Forward>
        </toolkit:NavigationOutTransition>
    </toolkit:TransitionService.NavigationOutTransition>
    
    
    
    <!--LayoutRoot est la grille racine où tout le contenu de la page est placé-->
    <Grid x:Name="LayoutRoot" Background="Transparent">

        <!--Contrôle Panorama-->
        <phone:Panorama Title="citations 365" SelectionChanged="MainPanoramaSelectionChanged" x:Name="MainPanorama">
            <phone:Panorama.TitleTemplate>
                <DataTemplate>
                    <TextBlock Text="citations 365" FontSize="160" Margin="0,0,0,0"/>
                </DataTemplate>
            </phone:Panorama.TitleTemplate>
            <phone:Panorama.Background>
                <ImageBrush x:Name="ImageBackground" 
                            ImageSource="/Citations365;component/Resources/Backgrounds/bg_blue.jpg" 
                            Opacity=".4"/>
            </phone:Panorama.Background>

            <!--Élément un de panorama-->
            <phone:PanoramaItem Header="aujourd'hui" Margin="-6,0,0,0">
                <phone:PanoramaItem.HeaderTemplate>
                    <DataTemplate>
                        <TextBlock Text="aujourd'hui" FontFamily="Segoe UI Light"
							FontSize="60" Style="{StaticResource PanoramaItemHeaderTextStyle}"/>
                    </DataTemplate>
                </phone:PanoramaItem.HeaderTemplate>

                <ScrollViewer Height="Auto" VerticalScrollBarVisibility="Hidden" Margin="0,-30,0,20">
                    <Grid>
                        <StackPanel Margin="0,0,0,20">
                            <Grid local:MetroInMotion.Tilt="1.5" Hold="Hold_dayquote">
                                <TextBlock x:Name="TBQuoteDay" Visibility="Visible" FontFamily="Segoe UI Black" FontSize="42"  Margin="12,0,0,0" HorizontalAlignment="Left" Height="Auto" TextWrapping="Wrap" VerticalAlignment="Top" Width="330" RenderTransformOrigin="0.5,0.5">
                                    <TextBlock.RenderTransform>
                                        <CompositeTransform/>
                                    </TextBlock.RenderTransform>
                                </TextBlock>
                            </Grid>
                            <Grid local:MetroInMotion.Tilt="1.5" Margin="0,0,0,10">
                                <TextBlock x:Name="TBAuthorDay" Tap="TBAuthorDay_Tap" FontFamily="Segoui UI Light" FontSize="22" Margin="12,0,0,0" Opacity="0.6" HorizontalAlignment="Left" Height="Auto" TextWrapping="Wrap" VerticalAlignment="Top" Width="330" RenderTransformOrigin="0.5,0.5">
                                    <TextBlock.RenderTransform>
                                        <CompositeTransform/>
                                    </TextBlock.RenderTransform>
                                </TextBlock>
                            </Grid>
                            <TextBlock x:Name="TBDateDay" FontFamily="Sego UI Light" FontSize="18" Margin="12,0,0,0" Opacity="0.6" HorizontalAlignment="Left" VerticalAlignment="Top" Height="Auto" Width="330" RenderTransformOrigin="0.5,0.5">
                                <TextBlock.RenderTransform>
                                    <CompositeTransform/>
                                </TextBlock.RenderTransform>
                            </TextBlock>

